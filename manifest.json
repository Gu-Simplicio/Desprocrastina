{
    "manifest_version": 2,
    "name": "Desprocrastina",
    "version": "1.0.1",
    

    "description": "Leva o usuário de uma página de distração para uma página de estudos",

    "content_scripts": [
        {
            "matches": ["*://*.youtube.com/*", "*://*.instagram.com/*", "*://*.tiktok.com/*", "*://*.x.com/*"],
            "js": ["desprocrastina.js"]
        }
    ]
}